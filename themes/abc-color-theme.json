{    
    "$schema": "vscode://schemas/color-theme",	
    "colors": {
        "editor.selectionBackground": "#FFDF24",
        "editor.selectionHighlightBackground": "#EFEFEF"
    },
	"tokenColors": [
        {
            "scope":"string.value",
            "settings": {
                "foreground": "#038153",     
            }
        },
        {
            "scope":"string.value.title.abc",
            "settings": {
                "foreground": "#038153",
                "fontStyle": "bold underline"     
            }
        },     
        {
            "scope":"keyword.other.instruction.abc",
            "settings": {
                "foreground": "#c75cd1",
                "fontStyle": ""  
            }
        },              
        {
            "scope": "keyword.other.information.abc",
            "settings": {
                "foreground": "#d057d4",
                "fontStyle": "bold underline"     
            }
        },
        {
            "scope":"keyword.other.ornaments.abc",
            "settings": {
                "foreground": "#038153",
                "fontStyle": ""  
            }
        },
        {
            "scope":"keyword.other.words.abc",
            "settings": {
                "foreground": "#AD5E18",  
                "fontStyle": "bold"   
            }
        },              
        {
            "scope":"keyword.other.chords.abc",
            "settings": {
                "foreground": "#1F73B7",     
            }
        },
        {
            "scope":"keyword.other.region.voice.abc",
            "settings": {
                "foreground": "#CC3340",     
                "fontStyle": "bold underline"
            }
        },
        {
            "scope":"keyword.other.bar.abc",
            "settings": {
                "foreground": "#CC3340",     
                "fontStyle": "bold"
            }
        },
        {
            "scope":"comment.line.abc",
            "settings": {
                "foreground": "#808080",     
                "fontStyle": "bold"
            }
        },
        {
            "scope":"annotation.inline.abc",
            "settings": {
                "foreground": "#d6e96c",     
                "fontStyle": "bold"
            }
        },
        {
            "scope":"keyword.other.symbols.abc",
            "settings": {
                "foreground": "#90430f",     
                "fontStyle": "bold"
            }
        },
    ]
}