{    
    "$schema": "vscode://schemas/color-theme",	
    "colors": {
        "editor.selectionBackground": "#f8eca8",
        "editor.selectionHighlightBackground": "#edf0e2",
        "editor.background": "#f3f5e1"

    },
	"tokenColors": [
        {
            "scope":"string.value",
            "settings": {
                "foreground": "#038153",     
            }
        },
        {
            "scope":"string.value.title.abc",
            "settings": {
                "foreground": "#038153",
                "fontStyle": "bold underline"     
            }
        },     
        {
            "scope":"keyword.other.instruction.abc",
            "settings": {
                "foreground": "#808080",
                "fontStyle": ""  
            }
        },              
        {
            "scope": "keyword.other.information.abc",
            "settings": {
                "foreground": "#038153",
            }
        },
        {
            "scope":"keyword.other.ornaments.abc",
            "settings": {
                "foreground": "#038153",
                "fontStyle": ""  
            }
        },
        {
            "scope":"keyword.other.words.abc",
            "settings": {
                "foreground": "#AD5E18",  
                "fontStyle": "bold"   
            }
        },              
        {
            "scope":"keyword.other.chords.abc",
            "settings": {
                "foreground": "#1F73B7",     
            }
        },
        {
            "scope":"keyword.other.region.voice.abc",
            "settings": {
                "foreground": "#CC3340",     
                "fontStyle": "bold underline"
            }
        },
        {
            "scope":"keyword.other.bar.abc",
            "settings": {
                "foreground": "#CC3340",     
                "fontStyle": "bold"
            }
        },
        {
            "scope":"comment.line.abc",
            "settings": {
                "foreground": "#808080",     
                "fontStyle": "bold"
            }
        },
        {
            "scope":"annotation.inline.abc",
            "settings": {
                "foreground": "#6d7c18",     
                "fontStyle": "bold"
            }
        },
        {
            "scope":"keyword.other.symbols.abc",
            "settings": {
                "foreground": "#90430f",     
                "fontStyle": "bold"
            }
        },
        {
            "scope":"keyword.other.accidentals.abc",
            "settings": {
                "foreground": "#90430f",     
                "fontStyle": "bold"
            }
        },
        {
            "scope":"keyword.other.ties.abc",
            "settings": {
                "foreground": "#35231836",     
                "fontStyle": "bold"
            }
        },
    ]
}